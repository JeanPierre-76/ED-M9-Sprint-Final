<div id="<%= dom_id orderdetail %>">
  <p>
    <strong>Date submitted:</strong>
    <%= orderdetail.date_submitted %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= orderdetail.status %>
  </p>

  <p>
    <strong>Subtotal:</strong>
    <%= orderdetail.subtotal %>
  </p>

  <p>
    <strong>Shipping:</strong>
    <%= orderdetail.shipping %>
  </p>

  <p>
    <strong>Tax:</strong>
    <%= orderdetail.tax %>
  </p>

  <p>
    <strong>Total:</strong>
    <%= orderdetail.total %>
