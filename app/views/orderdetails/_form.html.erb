<%= form_with(model: orderdetail) do |form| %>
  <% if orderdetail.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(orderdetail.errors.count, "error") %> prohibited this orderdetail from being saved:</h2>

      <ul>
        <% orderdetail.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :date_submitted, style: "display: block" %>
